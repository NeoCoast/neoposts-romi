- if controller_name != 'sessions'
  = link_to "Log in", new_session_path(resource_name), class: "btn btn-secondary login_alt_button"
  %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to "Sign up", new_registration_path(resource_name), class: "btn btn-secondary signup_alt_button"
  %br/

